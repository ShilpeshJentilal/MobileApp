<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txtnumberforps"
        android:layout_width="20dp"
        android:layout_height="40dp"
        android:layout_marginStart="4dp"
        android:paddingTop="5dp"
        android:text="1)"
        android:textAlignment="center"
        android:textColor="#070707"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.53" />

    <TextView
        android:id="@+id/txtModulesurveyforps"
        android:layout_width="91dp"
        android:layout_height="40dp"
        android:background="@drawable/psedit"
        android:paddingTop="5dp"
        android:text="Module "
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/txtnumberforps"
        app:layout_constraintTop_toTopOf="@+id/txtnumberforps" />

    <TextView
        android:id="@+id/txtmoudleenddateforps"
        android:layout_width="91dp"
        android:layout_height="40dp"
        android:background="@drawable/psedit"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:text="End Date"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/txtmodulstartdateforps"
        app:layout_constraintTop_toTopOf="@+id/txtmodulstartdateforps" />

    <TextView
        android:id="@+id/txtmodulstartdateforps"
        android:layout_width="91dp"
        android:layout_height="40dp"
        android:background="@drawable/psedit"
        android:paddingTop="5dp"
        android:text="Start Date"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="italic"
        app:layout_constraintStart_toEndOf="@+id/txtModulesurveyforps"
        app:layout_constraintTop_toTopOf="@+id/txtModulesurveyforps" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="84dp" />

    <Button
        android:id="@+id/btnEditps"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:background="@drawable/ic_baseline_edit_note_24"
        android:contentDescription="Edit"
        app:layout_constraintStart_toEndOf="@+id/txtmoudleenddateforps"
        app:layout_constraintTop_toTopOf="@+id/txtmoudleenddateforps" />

    <Button
        android:id="@+id/btnDeletePS"
        android:layout_width="38dp"
        android:layout_height="42dp"
        android:background="@android:drawable/ic_delete"
        android:contentDescription="Delete"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btnEditps"
        app:layout_constraintTop_toTopOf="@+id/btnEditps" />
</androidx.constraintlayout.widget.ConstraintLayout>